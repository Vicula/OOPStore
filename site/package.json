    "eslint": "^6.8.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^6.0.2",
    "husky": "^2.6.0",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "sass-loader": "^8.0.2"
  }
